#include <Arduino.h>
#include "mbedtls/pk.h"

int ret = 0;
mbedtls_pk_context pk;

void setup(){
    mbedtls_pk_init( &pk );
    /*
     * Read the RSA public key
     */
    if( ( ret = mbedtls_pk_parse_public_keyfile( &pk, "our-key.pub" ) ) != 0 )
    {
        printf( " failed\n  ! mbedtls_pk_parse_public_keyfile returned -0x%04x\n", -ret );
    }
}

void loop(){

}
